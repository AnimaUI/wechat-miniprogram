  height: 30rpx;
  border-radius: 50rpx;
  left: 50rpx;
  top: 40rpx;
  -webkit-animation: clouds 10s linear infinite;
  animation: clouds 10s linear infinite;
}
@-webkit-keyframes clouds {
  50% {
