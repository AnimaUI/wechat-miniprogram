page {
  background-color: #fff;
  overflow: hidden;
}
.github-box {
  --bgd-color: #cbe9f4;
  width: 88%;
  padding: 20rpx;
  border-radius: 100rpx;
  box-shadow: 0 18rpx 18rpx -14rpx var(--bgd-color);
  position: fixed;
  top: 455rpx;
  left: 50%;
  z-index: 1000;
  background: #fff;
  transform: translate(-50%, 0);
}
.github-box .item .describe {
  margin-top: 12rpx;
}
.github-box .item .describe text:first-child {
  margin-right: 12rpx;
}
#about {
  background: #fff;
  border-radius: 80rpx 80rpx 0 0;
  padding: 120rpx 40rpx 40rpx;
  height: calc(100vh - 600rpx);
  position: relative;
  top: 500rpx;
  z-index: 999;
  box-shadow: 0 20rpx 40rpx 10rpx var(--blackShadow);
  overflow: hidden;
}
#about .list {
  box-shadow: 0 14rpx 14rpx -14rpx rgba(0, 0, 0, 0.1);
  border-radius: 120rpx;
  margin-top: 36rpx;
}
#about .list .item {
  position: relative;
  display: flex;
  padding: 0 30rpx;
  min-height: 100rpx;
  background-color: var(--white);
  justify-content: space-between;
  align-items: center;
}
#about .list .item:after {
  position: absolute;
  top: 0;
  left: 0;
  box-sizing: border-box;
  width: 200%;
  height: 200%;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
  border-radius: inherit;
  content: '';
  transform: scale(0.5);
  transform-origin: 0 0;
  pointer-events: none;
}
#about .list .item .left {
  font-size: 30rpx;
}
#about .list .item .left text:last-child {
  margin-left: 12rpx;
}
.weather {
  --bgd-color: #cbe9f4;
  --light: #93d5eb;
  --medium: #66bbd8;
  --dark: #4da2bb;
  --bush: #ffffff;
  --trunk: #c2653c;
  --trunk2: #9d5d5d;
  --cloud: #ffffff;
  --sun: transparent;
  --rabbit: #ffffff;
  padding: 0;
  margin: 0;
  overflow: hidden;
  background-color: var(--bgd-color);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  width: 100%;
  height: 100vh;
  position: fixed;
  z-index: 1;
}
.container {
  position: absolute;
  width: 100%;
  height: 500rpx;
  top: 0;
  left: 0;
}
.container view {
  position: absolute;
  -webkit-transition: 4s ease background;
  transition: 4s ease background;
}
.sun {
  background-color: var(--sun);
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  top: 20rpx;
  left: 200rpx;
}
.rainbow {
  position: absolute;
  visibility: hidden;
  width: 75%;
  height: 270rpx;
  bottom: 82rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}
.rainbow.animated {
  -webkit-animation: rainbow 14s linear;
  animation: rainbow 14s linear;
  visibility: visible;
}
.rainbow .rainbow-colour {
  border-top-left-radius: 50% 100%;
  border-top-right-radius: 50% 100%;
  border-bottom: 0;
  left: 50%;
  -webkit-transform: translate(-50%, 0);
  transform: translate(-50%, 0);
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  bottom: 0;
  border-bottom: 0 !important;
}
.rainbow .rainbow-colour:nth-child(1) {
  border: 20rpx solid #ac01fd;
  height: 100%;
  width: 100%;
}
.rainbow .rainbow-colour:nth-child(2) {
  border: 20rpx solid #511ce0;
  height: 261rpx;
  width: 581rpx;
}
.rainbow .rainbow-colour:nth-child(3) {
  border: 20rpx solid #1a9fe2;
  height: 252rpx;
  width: 562rpx;
}
.rainbow .rainbow-colour:nth-child(4) {
  border: 20rpx solid #5ed81d;
  height: 243rpx;
  width: 543rpx;
}
.rainbow .rainbow-colour:nth-child(5) {
  border: 20rpx solid #f8f52a;
  height: 234rpx;
  width: 524rpx;
}
.rainbow .rainbow-colour:nth-child(6) {
  border: 20rpx solid #f7a528;
  height: 225rpx;
  width: 505rpx;
}
.rainbow .rainbow-colour:nth-child(7) {
  border: 20rpx solid #f63b2a;
  height: 216rpx;
  width: 486rpx;
}
@-webkit-keyframes rainbow {
  0% {
    opacity: 0;
  }
  10% {
    opacity: 0.5;
  }
  80% {
    opacity: 0.5;
  }
  100% {
    opacity: 0;
  }
}
@keyframes rainbow {
  0% {
    opacity: 0;
  }
  10% {
    opacity: 0.5;
  }
  80% {
    opacity: 0.5;
  }
  100% {
